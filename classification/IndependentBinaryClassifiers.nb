   RowBox[{
    SubscriptBox["P", "\[Beta]"], "\[Rule]", 
    RowBox[{"1", "-", 
     SubscriptBox["P", "\[Alpha]"]}]}], ",", 
   RowBox[{
    SubscriptBox["P", 
     RowBox[{"i_", ",", "\[Beta]", ",", "\[Alpha]"}]], "\[Rule]", 
