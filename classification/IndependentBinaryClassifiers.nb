 RowBox[{"desiredALabelPrevalence", "=", 
  RowBox[{"23", "/", "100"}]}], "\[IndentingNewLine]", 
 RowBox[{"datasetSize", "=", "10000"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"labelGroundTruth", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"RandomReal", "[", "]"}], "<", "desiredALabelPrevalence"}], 
       ",", "\[Alpha]", ",", "\[Beta]"}], "]"}], ",", 
     RowBox[{"{", "datasetSize", "}"}]}], "]"}]}], ";"}]}], "Input",
